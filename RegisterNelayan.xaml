<Window x:Class="JalaNota.RegisterNelayan"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JalaNota"
        mc:Ignorable="d"
        Title="RegisterNelayan" Height="450" Width="800">
    <Grid>
        <Label Content="ID Nelayan" HorizontalAlignment="Left" Margin="62,95,0,0" VerticalAlignment="Top" AutomationProperties.Name="labelNelayanId"/>
        <Label Content="Nama Nelayan" HorizontalAlignment="Left" Margin="62,137,0,0" VerticalAlignment="Top" AutomationProperties.Name="labelNamaNelayan"/>
        <Label Content="Username" HorizontalAlignment="Left" Margin="62,181,0,0" VerticalAlignment="Top" AutomationProperties.Name="labelUsn"/>
        <Label Content="Password" HorizontalAlignment="Left" Margin="62,222,0,0" VerticalAlignment="Top" AutomationProperties.Name="labelPass"/>
        <TextBox HorizontalAlignment="Left" Margin="184,99,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TextChanged="TextBox_TextChanged" AutomationProperties.Name="tbNelayanId"/>
        <TextBox HorizontalAlignment="Left" Margin="184,141,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TextChanged="TextBox_TextChanged" AutomationProperties.Name="tbNamaNelayan"/>
        <TextBox HorizontalAlignment="Left" Margin="184,185,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TextChanged="TextBox_TextChanged" AutomationProperties.Name="tbUsn"/>
        <PasswordBox HorizontalAlignment="Left" Margin="184,226,0,0" VerticalAlignment="Top" Width="120" AutomationProperties.Name="tbPassword"/>
        <Button Content="Add Nelayan" HorizontalAlignment="Left" Margin="140,290,0,0" VerticalAlignment="Top" Width="84" Background="#FF46C263" BorderBrush="#FF00A51C" Foreground="White" AutomationProperties.Name="btnAddNelayan"/>
        <Button Content="Cancel" HorizontalAlignment="Left" Margin="244,290,0,0" VerticalAlignment="Top" Width="60" Background="#FFFC3D39" BorderBrush="#FFA40300" Foreground="White" AutomationProperties.Name="btnCancel"/>

    </Grid>
</Window>
